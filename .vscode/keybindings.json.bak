[
  /* ==== QUICK OPEN & NAVIGATION ==== */
  {
    "command": "workbench.action.quickOpenSelectNext",
    "key": "ctrl+n",
    "when": "inQuickOpen && !inSearchEditor"
  },
  {
    "command": "workbench.action.quickOpenSelectPrevious",
    "key": "ctrl+p",
    "when": "inQuickOpen && !inSearchEditor"
  },
  {
    "key": "ctrl+shift+f",
    "command": "workbench.action.findInFiles",
    "when": "!terminalFocus"
  },
  {
    "command": "workbench.action.quickOpen",
    "key": "ctrl+f",
    "when": "!terminalFocus"
  },
  {
    "command": "workbench.action.findInFiles",
    "key": "shift+ctrl+f",
    "when": "!terminalFocus"
  },
  {
    "command": "workbench.action.terminal.focusFind",
    "key": "ctrl+shift+f",
    "when": "terminalFindFocused && terminalHasBeenCreated || terminalFindFocused && terminalProcessSupported || terminalFocusInAny && terminalHasBeenCreated || terminalFocusInAny && terminalProcessSupported"
  },
  {
    "command": "workbench.action.showCommands",
    "key": "ctrl+shift+p"
  },
  {
    "command": "search.action.focusNextSearchResult",
    "key": "ctrl+shift+n",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "command": "search.action.focusPreviousSearchResult",
    "key": "ctrl+shift+p",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "command": "editor.fold",
    "key": "tab",
    "when": "editorTextFocus && foldingEnabled && !inputFocus"
  },
  /* ==== WINDOW MANAGEMENT ==== */
  {
    "command": "workbench.action.switchWindow",
    "key": "ctrl+w w",
    "when": "!terminalFocus"
  },
  {
    "command": "workbench.action.splitEditor",
    "key": "ctrl+w '",
    "when": "!terminalFocus"
  },
  {
    "command": "workbench.action.splitEditorDown",
    "key": "ctrl+w 5",
    "when": "!terminalFocus"
  },
  {
    "command": "workbench.action.toggleZenMode",
    "key": "ctrl+'",
  },
  {
    "command": "workbench.action.toggleMaximizeEditorGroup",
    "key": "ctrl+shift+'",
    "when": "editorPartMaximizedEditorGroup || editorPartMultipleEditorGroups, || terminalFocus"
  },
  {
    "command": "workbench.action.closeActiveEditor",
    "key": "ctrl+q",
    "when": "!sideBarFocus"
  },

  /* ==== EDITOR NAVIGATION ==== */
  {
    "command": "workbench.action.navigateLeft",
    "key": "ctrl+h",
    "when": "editorTextFocus || terminalFocus || sideBarFocus || chatInputHasFocus"
  },
  {
    "command": "workbench.action.navigateRight",
    "key": "ctrl+l",
    "when": "editorTextFocus || terminalFocus || sideBarFocus || chatInputHasFocus"
  },
  {
    "command": "workbench.action.navigateUp",
    "key": "ctrl+k",
    "when": "editorTextFocus || terminalFocus || sideBarFocus || chatInputHasFocus"
  },
  {
    "command": "workbench.action.navigateDown",
    "key": "ctrl+j",
    "when": "editorTextFocus || terminalFocus || sideBarFocus || chatInputHasFocus"
  },

  /* ==== VIEW SIZE ADJUSTMENT ==== */
  {
    "command": "workbench.action.increaseViewWidth",
    "key": "ctrl+shift+l",
    "when": "editorTextFocus || terminalFocus || sideBarFocus || chatInputHasFocus"
  },
  {
    "command": "workbench.action.decreaseViewWidth",
    "key": "ctrl+shift+h",
    "when": "editorTextFocus || terminalFocus || sideBarFocus || chatInputHasFocus"
  },
  {
    "command": "workbench.action.increaseViewHeight",
    "key": "ctrl+shift+j",
    "when": "editorTextFocus || terminalFocus || sideBarFocus || chatInputHasFocus"
  },
  {
    "command": "workbench.action.decreaseViewHeight",
    "key": "ctrl+shift+k",
    "when": "editorTextFocus || terminalFocus || sideBarFocus || chatInputHasFocus"
  },

  /* ==== TAB NAVIGATION ==== */
  // {
  //   "command": "workbench.action.nextEditorInGroup",
  //   "key": "ctrl+o"
  // },
  // {
  //   "command": "workbench.action.previousEditorInGroup",
  //   "key": "ctrl+i"
  // },

  /* ==== SIDEBAR & PANELS ==== */
  {
    "command": "workbench.view.explorer",
    "key": "ctrl+e"
  },
  {
    "command": "workbench.action.toggleSidebarVisibility",
    "key": "ctrl+e",
    "when": "explorerViewletFocus || terminalFocus"
  },
  {
    "command": "workbench.action.toggleAuxiliaryBar",
    "key": "ctrl+shift+e"
  },
  {
    "command": "workbench.view.extensions",
    "key": "ctrl+d",
    "when": "viewContainer.workbench.view.extensions.enabled && sideBarFocus"
  },
  // {
  //   "command": "workbench.view.extension.project-manager",
  //   "key": "ctrl+p",
  //   "when": "viewContainer.workbench.view.extensions.enabled && sideBarFocus"
  // },
  {
    "command": "workbench.view.debug",
    "key": "ctrl+b",
    "when": "viewContainer.workbench.view.debug.enabled && sideBarFocus"
  },
  /* ==== TERMINAL ==== */
  // {
  //   "command": "workbench.action.togglePanel",
  //   "key": "ctrl+\\"
  // },
  {
    "command": "workbench.action.toggleMaximizedPanel",
    "key": "ctrl+'",
    "when": "terminalFocus && !editorTextFocus"
  },
  {
    "command": "workbench.action.terminal.toggleTerminal",
    "key": "ctrl+/",
    "when": "terminal.active"
  },
  {
    "command": "workbench.action.terminal.focusNext",
    "key": "ctrl+a n",
    "when": "terminalFocus && !editorTextFocus"
  },
  {
    "command": "workbench.action.terminal.new",
    "key": "ctrl+a c",
    "when": "terminalFocus && !editorTextFocus"
  },
  {
    "command": "workbench.action.terminal.kill",
    "key": "ctrl+a q",
    "when": "terminalFocus && !editorTextFocus"
  },

  /* ==== FILE EXPLORER ==== */
  {
    "command": "explorer.openAndPassFocus",
    "key": "o",
    "when": "explorerViewletFocus && explorerViewletVisible && !inputFocus"
  },

  {
    "command": "explorer.openToSide",
    "key": "shift+o",
    "when": "explorerViewletFocus && explorerViewletVisible && !inputFocus"
  },
  {
    "command": "openInExternalApp.open",
    "key": "ctrl+o",
    "when": "explorerViewletFocus && explorerViewletVisible && !inputFocus && resourceScheme == 'file'"
  },
  {
    "command": "revealFileInOS",
    "key": "ctrl+shift+o",
    "when": "explorerViewletFocus && explorerViewletVisible && !inputFocus"
  },

  /* ==== FILE OPERATIONS ==== */
  {
    "command": "explorer.newFile",
    "key": "n",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "command": "explorer.newFolder",
    "key": "shift+n",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "command": "renameFile",
    "key": "r",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "command": "deleteFile",
    "key": "d",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceReadonly && !inputFocus"
  },
  {
    "command": "filesExplorer.copy",
    "key": "y",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus"
  },
  {
    "command": "filesExplorer.cut",
    "key": "x",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus"
  },
  {
    "command": "filesExplorer.paste",
    "key": "p",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceReadonly && !inputFocus"
  },

  /* ==== EXPLORER NAVIGATION ==== */
  {
    "command": "list.collapse",
    "key": "h",
    "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  {
    "command": "list.focusDown",
    "key": "j",
    "when": "listFocus && explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  {
    "command": "list.focusUp",
    "key": "k",
    "when": "listFocus && explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  {
    "command": "list.toggleExpand",
    "key": "l",
    "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  // {
  //   "command": "list.toggleExpand",
  //   "key": "Enter",
  //   "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  // },
  {
    "command": "workbench.files.action.collapseExplorerFolders",
    "key": "z",
    "when": "listFocus && explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },

  /* ==== SUGGESTIONS & CODE ACTIONS ==== */
  {
    "command": "selectNextCodeAction",
    "key": "ctrl+n",
    "when": "codeActionMenuVisible"
  },
  {
    "command": "selectPrevCodeAction",
    "key": "ctrl+p",
    "when": "codeActionMenuVisible"
  },
  {
    "command": "acceptSelectedCodeAction",
    "key": "ctrl+y",
    "when": "neovim.mode == insert && codeActionMenuVisible"
  },
  // {
  //   "command": "editor.action.inlineSuggest.commit",
  //   "key": "ctrl+l",
  //   "when": "neovim.mode == insert && inputFocus"
  // },
  {
    "command": "editor.action.inlineSuggest.acceptNextWord",
    "key": "ctrl+l",
    "when": "inlineSuggestionVisible && !accessibilityModeEnabled && !editorReadonly"
  },
  {
    "command": "editor.action.inlineSuggest.acceptNextWord",
    "key": "ctrl+right",
    "when": "inlineSuggestionVisible && !accessibilityModeEnabled && !editorReadonly"
  },
  // {
  //   "command": "-tab",
  //   "key": "tab",
  //   "when": "neovim.mode == insert && editorTextFocus && !suggestWidgetVisible"
  // },

  /* ==== COPILOT ==== */
  // {
  //   "key": "ctrl+b c c",
  //   "command": "workbench.action.chat.openInEditor",
  //   "when": "chatEditingParticipantRegistered && chatIsEnabled && view != 'workbench.panel.chat.view.edits'"
  // },
  // {
  //   "key": "ctrl+b c e",
  //   "command": "workbench.action.chat.openEditSession",
  //   "when": "chatEditingParticipantRegistered && chatIsEnabled && view != 'workbench.panel.chat.view.edits'"
  {
    "command": "inlineChat.startWithCurrentLine",
    "key": "alt+i",
    "when": "editorFocus"
  },
  /* ==== HOVER NAVIGATION ==== */
  {
    "command": "editor.action.scrollDownHover",
    "key": "ctrl+n",
    "when": "editorHoverFocused"
  },
  {
    "command": "editor.action.scrollUpHover",
    "key": "ctrl+p",
    "when": "editorHoverFocused"
  },
  /* ==== LIST NAVIGATION ==== */
  {
    "command": "list.scrollDown",
    "key": "ctrl+n",
    "when": "listFocus && !inputFocus && !treestickyScrollFocused && listScrollAtBoundary != 'both' && listScrollAtBoundary != 'bottom'"
  },
  {
    "command": "list.scrollUp",
    "key": "ctrl+p",
    "when": "listFocus && !inputFocus && !treestickyScrollFocused && listScrollAtBoundary != 'both' && listScrollAtBoundary != 'top'"
  },
  /* ==== FILE BROWSER ==== */
  {
    "command": "file-browser.stepIn",
    "key": "ctrl+l",
    "when": "inFileBrowser"
  },
  {
    "command": "file-browser.stepOut",
    "key": "ctrl+h",
    "when": "inFileBrowser"
  },
  /* ==== PROJECT MANAGEMENT ==== */
  {
    "command": "workbench.action.zoomOut",
    "key": "ctrl+-"
  },
  {
    "command": "workbench.action.zoomIn",
    "key": "ctrl+="
  },
  {
    "command": "workbench.action.zoomReset",
    "key": "ctrl+0"
  },
  {
    "command": "workbench.action.reloadWindow",
    "key": "ctrl+x r"
  },
  {
    "command": "workbench.action.clearRecentFiles",
    "key": "ctrl+x backspace"
  },
  /* ==== DISABLED BINDINGS ==== */
  {
    "command": "-workbench.action.files.newUntitledFile",
    "key": "ctrl+n"
  },
  {
    "command": "-vscode-neovim.ctrl-f",
    "key": "ctrl+f",
    "when": "editorTextFocus && neovim.ctrlKeysNormal.f && neovim.init && neovim.mode != 'insert'"
  },
  {
    "command": "-editor.action.pageDownHover",
    "key": "ctrl+f",
    "when": "editorHoverFocused"
  },
  {
    "command": "-vscode-neovim.send",
    "key": "ctrl+f",
    "when": "editorTextFocus && neovim.ctrlKeysInsert.f && neovim.init && neovim.mode == 'insert'"
  },
  {
    "command": "-workbench.action.terminal.focusFind",
    "key": "ctrl+f",
    "when": "terminalFindFocused && terminalHasBeenCreated || terminalFindFocused && terminalProcessSupported || terminalFocusInAny && terminalHasBeenCreated || terminalFocusInAny && terminalProcessSupported"
  },
  {
    "command": "-workbench.action.findInFiles",
    "key": "shift+cmd+f"
  },
  {
    "command": "-workbench.action.quickOpen",
    "key": "cmd+p"
  },
  {
    "command": "-file-browser.stepIn",
    "key": "right",
    "when": "inFileBrowser"
  },
  {
    "command": "-file-browser.stepOut",
    "key": "left",
    "when": "inFileBrowser"
  },
  {
    "command": "-television.ToggleFileFinder",
    "key": "ctrl+p"
  },
  {
    "command": "-television.ToggleTextFinder",
    "key": "ctrl+shift+f"
  },
  {
    "command": "-projectManager.listProjects",
    "key": "shift+alt+p"
  },
  {
    "command": "-workbench.action.terminal.openNativeConsole",
    "key": "ctrl+shift+c",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+l",
    "command": "-workbench.action.chat.newChat",
    "when": "chatIsEnabled && inChat"
  },
  {
    "key": "ctrl+shift+m",
    "command": "-editor.action.toggleTabFocusMode"
  },
  {
    "key": "ctrl+enter",
    "command": "inlineChat.acceptChanges",
    "when": "inlineChatHasProvider && inlineChatVisible"
  },
  {
    "key": "cmd+enter",
    "command": "-inlineChat.acceptChanges",
    "when": "inlineChatHasProvider && inlineChatVisible"
  },
  {
    "key": "ctrl+shift+c",
    "command": "-claude-code-chat.openChat"
  }
]
